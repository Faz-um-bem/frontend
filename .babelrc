{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    ["babel-plugin-root-import", {
      "paths": [
        {
          "rootPathSuffix": "./src",
          "rootPathPrefix": "~/"
        }
      ]
    }],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ]
}
